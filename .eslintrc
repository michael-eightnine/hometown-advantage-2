{
  "extends": "react-app",
  "rules": {
    "comma-dangle": [1,"always-multiline"],
    "no-dupe-keys": 1,
    "quotes": [1,"single","avoid-escape"],
    "arrow-parens": [1,"always"],
    "arrow-spacing": [1,{"before":true,"after":true}],
    "no-var": 1,
    "prefer-const": 1,
    "semi": ["error", "never"]
  }
}
